{
  "name": "range-pool",
  "version": "3.1.0",
  "description": "A base library for creating range-based worker pools",
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-steelbrain": "^5.0.0",
    "eslint-config-steelbrain": "^3.0.0",
    "flow-bin": "^0.47.0"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean; babel src --out-dir lib",
